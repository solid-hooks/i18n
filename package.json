{
  "name": "@solid-hooks/i18n",
  "type": "module",
  "version": "0.1.2",
  "description": "i18n for solid-js",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/solid-hooks/i18n",
  "bugs": "https://github.com/solid-hooks/i18n/issue",
  "keywords": [
    "typescript"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      },
      "require": {
        "types": "./dist/utils.d.cts",
        "default": "./dist/utils.cjs"
      }
    },
    "./vite": {
      "import": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "require": {
        "types": "./dist/vite.d.cts",
        "default": "./dist/vite.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite serve playground",
    "lint": "eslint . --max-warnings 0",
    "format": "eslint . --fix",
    "build": "tsup",
    "release": "pnpm lint && pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "solid-js": "^1"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "^2.3.3",
    "@subframe7536/type-utils": "^0.1.6",
    "object-path-access": "^4.0.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.6.1",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-solid": "^0.13.2",
    "jsdom": "^24.0.0",
    "solid-js": "^1.8.17",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.5.1",
    "yaml": "^2.4.1"
  }
}
