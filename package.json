{
  "name": "@solid-hooks/i18n",
  "type": "module",
  "version": "0.2.3",
  "description": "i18n for solid-js",
  "author": "subframe7536",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-hooks/i18n.git"
  },
  "bugs": "https://github.com/solid-hooks/i18n/issue",
  "keywords": [
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./message": {
      "import": "./dist/message.js",
      "require": "./dist/message.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx vite serve playground",
    "lint": "eslint . --max-warnings 0 --fix",
    "format": "eslint . --fix",
    "build": "tsup",
    "release": "pnpm lint && pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run",
    "test:build": "bunx vite build playground"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "solid-js": ">=1.6"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "^2.3.3",
    "@subframe7536/type-utils": "^0.1.6",
    "object-path-access": "^4.0.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.9.7",
    "@types/node": "^22.10.1",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "eslint-plugin-solid": "^0.14.4",
    "jsdom": "^25.0.1",
    "solid-js": "^1.9.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-solid": "^2.11.0",
    "vitest": "^2.1.8",
    "yaml": "^2.6.1"
  }
}
