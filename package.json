{
  "name": "@solid-hooks/i18n",
  "type": "module",
  "version": "0.2.0",
  "description": "i18n for solid-js",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/solid-hooks/i18n",
  "bugs": "https://github.com/solid-hooks/i18n/issue",
  "keywords": [
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./message": {
      "import": "./dist/message.js",
      "require": "./dist/message.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/utils.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunx vite serve playground",
    "lint": "eslint . --max-warnings 0 --fix",
    "format": "eslint . --fix",
    "build": "tsup",
    "release": "pnpm lint && pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "solid-js": ">=1.6"
  },
  "dependencies": {
    "@solid-primitives/event-listener": "^2.3.3",
    "@subframe7536/type-utils": "^0.1.6",
    "local-pkg": "^0.5.0",
    "object-path-access": "^4.0.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.7.4",
    "@types/node": "^20.14.14",
    "bumpp": "^9.4.2",
    "eslint": "^9.8.0",
    "eslint-plugin-solid": "^0.14.1",
    "jsdom": "^24.1.1",
    "solid-js": "^1.8.19",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.0.5",
    "yaml": "^2.5.0"
  }
}
